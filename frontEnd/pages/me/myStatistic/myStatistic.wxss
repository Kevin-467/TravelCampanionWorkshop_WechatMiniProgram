page {
    background:linear-gradient(to bottom,#dafbda,#ffffff);
    color:#333;
}

.tab-bar {
    display: flex;
    justify-content: space-around;
    height: 100rpx;
    position: relative;
    background: #fff;
    border-bottom: 2rpx solid #999;
}

.nav-item {
    display: flex;
    align-items: center;
    justify-content: center;
    border:4rpx solid transparent
}

.active {
    color:#4caf50;
    border-bottom:4rpx solid #4caf50;
}

.conatnier {
    flex:1;
}